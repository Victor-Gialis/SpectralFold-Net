{
  "dataset": {
    "name": "CWRU",
    "fault_filter": [
      "normal",
      "ball",
      "inner",
      "outer"
    ],
    "speed_filter": [
      "1730",
      "1750",
      "1772",
      "1797"
    ],
    "transform_type": "psd_envelope",
    "window_size": 4096,
    "stride": 256,
    "flip": true,
    "downsample_factor": 2
  },
  "dataloader": {
    "batch_size": 256,
    "shuffle": true
  },
  "model": {
    "encoder_dim": 128,
    "decoder_dim": 256,
    "patch_size": 64,
    "n_layers": 4,
    "heads": 8,
    "dropout": 0.4
  },
  "training": {
    "epochs": 100,
    "learning_rate": 1e-05,
    "model_name": "model_v1.2"
  }
}