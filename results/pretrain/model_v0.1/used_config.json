{
  "dataset": {
    "name": "CWRU",
    "fault_filter": null,
    "speed_filter": null,
    "transform_type": "psd",
    "window_size": 2048,
    "stride": 256,
    "flip": false,
    "downsample_factor": 2
  },
  "dataloader": {
    "batch_size": 256,
    "shuffle": true
  },
  "model": {
    "encoder_dim": 128,
    "decoder_dim": 256,
    "patch_size": 64,
    "n_layers": 4,
    "heads": 8,
    "dropout": 0.4
  },
  "training": {
    "epochs": 100,
    "normalization": "z",
    "learning_rate": 1e-05,
    "model_name": "model_v0.1"
  }
}