{
  "dataset": {
    "name": "CWRU",
    "fault_filter": [
      "normal",
      "inner",
      "outer",
      "ball"
    ],
    "speed_filter": [
      "1730",
      "1797"
    ],
    "transform_type": "psd",
    "window_size": 16384,
    "stride": 819,
    "downsample_factor": 2
  },
  "dataloader": {
    "batch_size": 32,
    "shuffle": true
  },
  "model": {
    "encoder_dim": 1024,
    "decoder_dim": 2048,
    "patch_size": 128,
    "n_layers": 1,
    "heads": 4,
    "dropout": 0.1
  },
  "training": {
    "epochs": 5000,
    "learning_rate": 1e-05,
    "model_name": "model_v5.1"
  }
}