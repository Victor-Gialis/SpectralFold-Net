{
  "dataset": {
    "fault_filter": ["normal", "inner", "outer"],
    "window_size": 2048,
    "stride": 512,
    "downsample_factor": 2,
    "envelope_analysis": false
  },
  "dataloader": {
    "batch_size": 32,
    "shuffle": true
  },
  "model": {
    "encoder_dim": 512,
    "decoder_dim": 1024,
    "patch_size": 128,
    "n_layers": 2,
    "heads": 4,
    "dropout": 0.1
  },
  "training": {
    "epochs": 1,
    "learning_rate": 0.0001,
    "model_name": "model_v1"
  }
}