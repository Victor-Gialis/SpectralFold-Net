{
  "dataset": {
    "name":"CWRU",
    "fault_filter": ["normal"],
    "speed_filter": ["1730","1797"],
    "transform_type":"psd_envelope",
    "window_size": 16384,
    "stride": 512,
    "downsample_factor": 2
  },
  "dataloader": {
    "batch_size": 32,
    "shuffle": true
  },
  "model": {
    "encoder_dim": 1024,
    "decoder_dim": 4096,
    "patch_size": 64,
    "n_layers": 2,
    "heads": 4,
    "dropout": 0.1
  },
  "training": {
    "epochs": 32,
    "learning_rate": 1e-5,
    "model_name": "model_v4.1"
  }
}